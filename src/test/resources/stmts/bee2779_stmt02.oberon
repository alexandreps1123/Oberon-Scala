MODULE bee2779;

VAR
  a : ARRAY 6 OF INTEGER;
  cnt : ARRAY 5 OF INTEGER;
  v, answer, n, m : INTEGER;

BEGIN
  a[0] := 3;
  a[1] := 3;
  a[2] := 2;
  a[3] := 3;
  a[4] := 3;
  a[5] := 3;

  cnt[0] := 0;
  cnt[1] := 0;
  cnt[2] := 0;
  cnt[3] := 0;
  cnt[4] := 0;
  
  n := 5;
  m := 6;

  answer := n;

  FOREACH v IN a
    cnt[v] := cnt[v] + 1;

    IF (cnt[v] = 1) THEN
      answer := answer - 1
    END
  END
END

END bee2779.